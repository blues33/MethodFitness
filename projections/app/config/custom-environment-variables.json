{
  "configs": {
    "children": {
      "eventstore": {
        "http": "EVENTSTORE_HTTP",
        "host": "EVENTSTORE_HOST",
        "maxRetries": "EVENTSTORE_MAX_RETRIES",
        "maxReconnections": "EVENTSTORE_MAX_RECONNECTIONS",
        "verbose": "EVENTSTORE_VERBOSE",
        "systemUsers": {
          "admin": "EVENTSTORE_USER",
          "adminPassword": "EVENTSTORE_PASSWORD"
        }
      },
      "postgres": {
        "config": {
          "user": "POSTGRES_USER",
          "database": "POSTGRES_DATABASE",
          "host": "POSTGRES_HOST",
          "password": "POSTGRES_PASSWORD",
          "port": "POSTGRES_PORT"
        }
      }
    }
  }
}
