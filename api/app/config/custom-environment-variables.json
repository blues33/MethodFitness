{
  "app": {
    "env": "NODE_ENV",
    "swagger_ui_url": "SWAGGER_UI_URL",
    "logging_level": "LOGGING_LEVEL",
    "logging_transports": "LOGGING_TRANSPORTS",
    "host": "API_HOST",
    "port": "API_PORT",
    "frontEndHost": "FRONT_END_HOST",
    "frontEndHostAlt": "FRONT_END_HOST_ALT",
    "frontEndPort": "FRONT_END_PORT"

  },
  "configs": {
    "children": {
      "eventstore": {
        "host": "EVENTSTORE_HOST",
        "http": "EVENTSTORE_HTTP",
        "systemUsers": {
          "admin": "EVENTSTORE_USER",
          "adminPassword": "EVENTSTORE_PASSWORD"
        }
      },
      "postgres": {
        "config" :{
          "user": "POSTGRES_USER",
          "database": "POSTGRES_DATABASE",
          "host"     : "POSTGRES_HOST",
          "password": "POSTGRES_PASSWORD",
          "port": "POSTGRES_PORT"
        }
      },
      "loggly": {
        "token": "LOGGLY_TOKEN"
      }
    }
  }
}
